{
    "openapi":"3.0.2",
    "info": {
      "title":"To-Do list for power grid workers",
      "version":"1.0"
    },
    "servers": [
      {"url":"https://localhost:3000"}
    ],
    "paths": {
        "/users": {
            "get": {
                "description": "Get all users",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "List of all users",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "users": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "number",
                                                        "example": "1"
                                                    },
                                                    "lat": {
                                                        "type": "number",
                                                        "example": "53.72"
                                                    },
                                                    "lng": {
                                                        "type": "number",
                                                        "example": "27.42"
                                                    },
                                                    "note": {
                                                        "type": "string",
                                                        "example": "Wires on the ground"
                                                    },
                                                    "completion": {
                                                        "type": "boolean",
                                                        "example": "false"
                                                    }

                                                }
                                            }
                                        }

                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
  }